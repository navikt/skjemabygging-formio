{
  "extends": "react-app",
  "globals": {
    "fetchMock": false,
    "vi": false
  },
  "rules": {
    "no-unused-labels": "error",
    "no-unused-vars": "off",
    "no-duplicate-imports": "error",
    "@typescript-eslint/no-unused-vars": ["error", { "args": "none" }],
    "import/no-internal-modules": ["error", {
      "forbid": [
        "@navikt/skjemadigitalisering-shared-@(components|domain)/**/!(*.less)"
      ]
    }]
  }
}
