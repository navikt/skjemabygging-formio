{
  "name": "skjemautfylling-formio",
  "homepage": "/skjema",
  "proxy": "http://localhost:8080",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "cracoConfig": "craco.config.cjs",
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "craco-less": "^1.16.0",
    "eventemitter2": "^6.4.3",
    "formiojs": "^4.9.20-rc.1",
    "http-proxy-middleware": "^1.0.5",
    "lodash.throttle": "^4.1.1",
    "nav-datovelger": "^6.1.9",
    "nav-frontend-core": "^4.0.13",
    "nav-frontend-js-utils": "^1.0.9",
    "nav-frontend-knapper": "^2.0.9",
    "nav-frontend-knapper-style": "^1.0.1",
    "nav-frontend-lenker-style": "^0.2.29",
    "nav-frontend-paneler": "^2.0.10",
    "nav-frontend-paneler-style": "^0.3.21",
    "nav-frontend-skjema-style": "^2.0.9",
    "nav-frontend-stegindikator-style": "^1.0.31",
    "nav-frontend-typografi": "^2.0.18",
    "nav-frontend-typografi-style": "^1.0.19",
    "pusher": "^3.0.1",
    "react": "^16.13.1",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "skjemapublisering": "navikt/skjemapublisering-test.git#e4a5bc6d4bd0dff60370ea4fea6b2c8fae84b727"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "REACT_APP_GIT_VERSION=`git describe --always --match 'NOT A TAG' --abbrev=0 --dirty` craco build",
    "test": "craco test",
    "eject": "craco eject",
    "pretest": "npm run postinstall",
    "prestart": "npm run postinstall",
    "prebuild": "npm run postinstall",
    "install-template": "npm --prefix ./template install ./template",
    "build-template": "cd template && npm run build",
    "postinstall": "npm run install-template && npm run build-template",
    "install-all": "npm install && npm --prefix ./server install ./server",
    "dev-server": "cd server && DECORATOR_URL=\"https://www.nav.no/dekoratoren/\" npm start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
