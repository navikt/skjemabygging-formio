{
  "name": "@navikt/skjemabryggeriet",
  "version": "1.0.0",
  "private": true,
  "description": "Repository for skjemabygger og skjemautfyller",
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/skjemabygging-formio.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "check-types": "lerna run check-types",
    "clean": "lerna run clean && lerna clean -y",
    "bootstrap": "yarn --prefer-offline && lerna bootstrap",
    "watch": "lerna run --parallel watch",
    "build": "lerna run build",
    "prepare": "husky install",
    "start": "lerna run start --stream",
    "start:bygger": "lerna run --scope @navikt/bygger --scope @navikt/bygger-backend start --stream",
    "start:fyllut": "lerna run --scope @navikt/fyllut-frontend --scope @navikt/fyllut-backend start --stream",
    "test": "cross-env-shell CI=true lerna run --stream test",
    "test:coverage": "cross-env-shell CI=true lerna run --stream test:coverage",
    "cypress": "lerna run --stream cypress",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs",
    "lint:staged": "lint-staged"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-v8": "^0.34.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-vitest": "^0.2.8",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "husky": "^8.0.3",
    "lerna": "^6.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
