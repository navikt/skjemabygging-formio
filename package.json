{
  "name": "@navikt/skjemabryggeriet",
  "version": "1.0.0",
  "private": true,
  "description": "Repository for skjemabygger og skjemautfyller",
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/skjemabygging-formio.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "check-types": "lerna run check-types",
    "clean": "lerna run clean && lerna clean -y",
    "bootstrap": "yarn --prefer-offline && lerna bootstrap",
    "watch": "lerna run --parallel watch",
    "build": "lerna run build",
    "prepare": "husky install",
    "start": "lerna run start --stream",
    "start:bygger": "lerna run --scope @navikt/bygger --scope @navikt/bygger-backend start --stream",
    "start:fyllut": "lerna run --scope @navikt/fyllut-frontend --scope @navikt/fyllut-backend start --stream",
    "test": "cross-env-shell CI=true lerna run --stream test",
    "test:coverage": "cross-env-shell CI=true lerna run --stream test:coverage",
    "cypress": "lerna run --stream cypress",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs",
    "lint:staged": "lint-staged"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-vitest": "^0.2.6",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "formiojs": "4.14.6",
    "husky": "^7.0.4",
    "lerna": "^6.6.1",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=16.17.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
