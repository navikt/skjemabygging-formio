{
  "name": "@navikt/skjemabryggeriet",
  "version": "1.0.0",
  "description": "Repository for skjemabygger og skjemautfyller",
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/skjemabygging-formio.git"
  },
  "private": true,
  "scripts": {
    "clean": "lerna run clean && lerna clean -y",
    "bootstrap": "yarn && lerna bootstrap",
    "watch": "lerna run --parallel watch",
    "build": "lerna run build",
    "build:scripts": "lerna exec --scope @navikt/skjemadigitalisering-scripts -- yarn install --frozen-lockfile",
    "start": "lerna run start",
    "start:bygger": "lerna run --scope @navikt/bygger --scope @navikt/bygger-backend start",
    "start:fyllut": "lerna run --scope @navikt/fyllut-frontend --scope @navikt/fyllut-backend start",
    "test": "cross-env-shell CI=true lerna run --stream test",
    "test:coverage": "cross-env-shell CI=true lerna run --stream test:coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs"
  },
  "devDependencies": {
    "@material-ui/styles": "4.10.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-eslint": "^10.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "formiojs": "4.14.0-rc.18",
    "husky": "^3.1.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.15.0 <=14.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
