<div class="day" role="group"
  {% if (ctx.component.hideLabel) { %}
  aria-label="{{ctx.component.label}}"
  {% } else { %}
  aria-labelledby="l-{{ctx.instance.id}}-{{ctx.component.key}}"
  {% } %}
  {% if (ctx.component.description) { %}
  aria-describedby="d-{{ctx.instance.id}}-{{ctx.component.key}}"
  {% } %}
  >
  {% if (ctx.dayFirst && ctx.showDay) { %}
  <div>
    {% if (!ctx.component.hideInputLabels) { %}
    <label for="{{ctx.component.key}}-day" class="aksel-label">
      {{ctx.t('Dag')}}
      {% if(ctx.component.fields.day.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {% } %}
    <div class="{% if (ctx.component.fields.day.fieldSize) { %} {{ctx.component.fields.day.fieldSize}}{% } %}">{{ctx.day}}</div>
  </div>
  {% } %}
  {% if (ctx.showMonth) { %}
  <div>
    {% if (!ctx.component.hideInputLabels) { %}
    <label for="{{ctx.component.key}}-month" class="aksel-label">
      {{ctx.t('Måned')}}
      {% if(ctx.component.fields.month.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {% } %}
    <div class="{% if (ctx.component.fields.month.fieldSize) { %} {{ctx.component.fields.month.fieldSize}}{% } %} ">{{ctx.month}}</div>
  </div>
  {% } %}
  {% if (!ctx.dayFirst && ctx.showDay) { %}
  <div>
    {% if (!ctx.component.hideInputLabels) { %}
    <label for="{{ctx.component.key}}-day" class="aksel-label">
      {{ctx.t('Dag')}}
      {% if(ctx.component.fields.day.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {% } %}
    <div class="{% if (ctx.component.fields.day.fieldSize) { %} {{ctx.component.fields.day.fieldSize}}{% } %}">{{ctx.day}}</div>
  </div>
  {% } %}
  {% if (ctx.showYear) { %}
  <div>
    {% if (!ctx.component.hideInputLabels) { %}
    <label for="{{ctx.component.key}}-year" class="aksel-label">
      {{ctx.t('År')}}
      {% if(ctx.component.fields.year.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {% } %}
    <div class="{% if (ctx.component.fields.year.fieldSize) { %} {{ctx.component.fields.year.fieldSize}}{% } %}">{{ctx.year}}</div>
  </div>
  {% } %}
</div>
<input name="ctx.data[day]" type="hidden" class="form-control{% if (ctx.component.fieldSize) { %} {{ctx.component.fieldSize}}{% } %}" value="" ref="input">

