<div data-cy="error-summary" id="error-summary" class="aksel-error-summary aksel-error-summary--medium" tabindex="-1">
  <h2 tabindex="-1" class="aksel-error-summary__heading aksel-heading aksel-heading--small">For å gå videre må du rette opp følgende:</h2>
  <ul class="aksel-error-summary__list aksel-body-short aksel-body-short--medium">
    {% ctx.errors.forEach(function(err) { %}
    <li>
      {{ err.message }}
    </li>
    {% }) %}
  </ul>
</div>

