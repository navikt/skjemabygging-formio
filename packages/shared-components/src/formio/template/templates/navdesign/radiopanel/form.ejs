<div role="group" class="aksel-radio-group aksel-radio-group--medium" aria-labelledby="l-{{ctx.instance.id}}-{{ctx.component.key}}" aria-describedby="e-{{ctx.instance.id}}-{{ctx.component.key}} {% if (ctx.component.description) { %}d-{{ctx.instance.id}}-{{ctx.component.key}}{% } %}">
  <div class="label-track-changes">
    <label class="aksel-label" id="l-{{ctx.instance.id}}-{{ctx.component.key}}">
      {{ctx.t(ctx.component.label)}}
      {% if(ctx.component.validate.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {{ctx.util.getBuilderTags(ctx)}}
  </div>
  {% if (ctx.component.description && ctx.component.descriptionPosition !== "above") { %}
    <div class="description" id="d-{{ctx.instance.id}}-{{ctx.component.key}}">{{ctx.t(ctx.component.description)}}</div>
  {% } %}
  <div class="aksel-radio-buttons">
  {% ctx.values.forEach(function(item) { %}
  <div class="aksel-radio aksel-radio--medium" ref="wrapper">
    <{{ctx.input.type}}
      class="aksel-radio__input radioknapp"
      ref="input"
      {% for (var attr in ctx.input.attr) { %}
      {{attr}}="{{ctx.input.attr[attr]}}"
      {% } %}
      value="{{item.value}}"
      {% if (ctx.value && (ctx.value === item.value || (typeof ctx.value === 'object' && ctx.value.hasOwnProperty(item.value) && ctx.value[item.value]))) { %}
      checked=true
      {% } %}
      {% if (item.disabled) { %}
      disabled=true
      {% } %}
      id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
    >
    <label class="aksel-radio__label" for="{{ctx.id}}{{ctx.row}}-{{item.value}}">{{ctx.t(item.label)}} </label>
  </div>
  {% }) %}
  </div>
</div>

