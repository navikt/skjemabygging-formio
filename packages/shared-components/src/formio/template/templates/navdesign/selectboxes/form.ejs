<div role="group" class="aksel-checkbox-group aksel-checkbox-group--medium" aria-labelledby="l-{{ctx.instance.id}}-{{ctx.component.key}}">
  <div class="label-track-changes">
    <label class="aksel-label" id="l-{{ctx.instance.id}}-{{ctx.component.key}}">
      {{ctx.t(ctx.component.label)}}
      {% if(ctx.component.validate.required === false) { %}
        ({{ctx.t("valgfritt")}})
      {% } %}
    </label>
    {{ctx.util.getBuilderTags(ctx)}}
  </div>
  {% if (ctx.component.description && ctx.component.descriptionPosition !== "above" ) { %}
    <div class="description">{{ctx.t(ctx.component.description)}}</div>
  {% } %}
  <div class="aksel-checkboxes">
  {% ctx.values.forEach(function(item) { %}
    <div class="aksel-checkbox aksel-checkbox--medium form-check checkbox">
      <input
        ref="input"
        type="checkbox"
        id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
        class="aksel-checkbox__input checkboks"
        name="data[{{ctx.key}}][{{item.value}}]"
        {% for (var attr in ctx.input.attr) { %}
        {{attr}}="{{ctx.input.attr[attr]}}"
        {% } %}
        aria-checked="{{ctx.value[item.value] === true}}"
        aria-describedby="{{ctx.key}}-error"
        value="{{item.value}}"
        {% if (ctx.value[item.value] === true) { %}
        checked
        {% } %}
        {% if (item.disabled) { %}
        disabled
        {% } %}
      />
      <label class="aksel-checkbox__label aksel-body-short" for="{{ctx.id}}{{ctx.row}}-{{item.value}}">
        <span class="aksel-checkbox__icon">
          <svg xmlns="http://www.w3.org/2000/svg" width="13" height="10" viewBox="0 0 13 10" fill="none" focusable="false" role="img" aria-hidden="true"><path d="M4.03524 6.41478L10.4752 0.404669C11.0792 -0.160351 12.029 -0.130672 12.5955 0.47478C13.162 1.08027 13.1296 2.03007 12.5245 2.59621L5.02111 9.59934C4.74099 9.85904 4.37559 10 4.00025 10C3.60651 10 3.22717 9.84621 2.93914 9.56111L0.439143 7.06111C-0.146381 6.47558 -0.146381 5.52542 0.439143 4.93989C1.02467 4.35437 1.97483 4.35437 2.56036 4.93989L4.03524 6.41478Z" fill="currentColor"></path></svg>
        </span>
        <span class="aksel-checkbox__content">
          <span class="aksel-checkbox__label-text aksel-body-short aksel-body-short--medium">
            {{ctx.t(item.label)}}
          </span>
        </span>
      </label>
    </div>
  {% }) %}
  </div>
</div>

