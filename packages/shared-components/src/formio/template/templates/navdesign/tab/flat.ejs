{% ctx.component.components.forEach(function(tab, index) { %}
  <div class="card border">
    <div class="card-header bg-default">
      <h4 class="card-title">{{ ctx.t(tab.label) }}</h4>
    </div>
    <div
      class="card-body"
      ref="{{ctx.tabKey}}"
    >
      {{ ctx.tabComponents[index] }}
    </div>
  </div>
{% }) %}
