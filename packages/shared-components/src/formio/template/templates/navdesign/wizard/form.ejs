<div class="{{ ctx.className }}">
  <div>
    <section id="maincontent" class="fyllut-layout" tabIndex="-1">
      <div class="main-col">
        <h2 id="form-id" class="mb navds-heading navds-heading--large">
          {{ ctx.t(ctx.panels[ctx.currentPage].title) }}
        </h2>
        <form aria-labelledby="form-id">
          <div ref="{{ ctx.wizardKey }}" class="mb-14">
            {{ ctx.components }}
          </div>
          <div class="mb-4">
            {{ ctx.wizardNav }}
          </div>
        </form>
      </div>
      <div class="right-col">
        {{ ctx.wizardHeader }}
      </div>
    </section>
  </div>
</div>
