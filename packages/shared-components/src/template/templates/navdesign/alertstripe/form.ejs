{{ctx.util.getDiffTag(ctx)}}
<div class="navds-alert navds-alert--{{ctx.component.alerttype === 'suksess' ? 'success' : (ctx.component.alerttype === 'advarsel' ? 'warning' : (ctx.component.alerttype === 'feil' ? 'error' : ctx.component.alerttype))}}{{ctx.component.isInline ? ' navds-alert--inline' : ''}}" ref="html">
    <span class="sr-only" id="alert-{{ctx.instance.id}}-{{ctx.component.key}}">{{ctx.component.alerttype}}</span>
    <svg width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" focusable="false" role="img" class="navds-alert__icon" aria-labelledby="alert-{{ctx.instance.id}}-{{ctx.component.key}}">
        {% if (ctx.component.alerttype === "info") { %}
        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0ZM9 19v-2h2v-5H9v-2h4v7h2v2H9Zm3-14a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3Z" fill="currentColor"></path>
        {% } %}
        {% if (ctx.component.alerttype === "success" || ctx.component.alerttype === "suksess") { %}
        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0Zm5.047 7.671 1.399 1.43-8.728 8.398L6 14.02l1.395-1.434 2.319 2.118 7.333-7.032Z" fill="currentColor"></path>
        {% } %}
        {% if (ctx.component.alerttype === "warning" || ctx.component.alerttype === "advarsel") { %}
        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 0a1 1 0 0 1 .894.553l11 22A1 1 0 0 1 23 24H1a1 1 0 0 1-.894-1.447l11-22A1 1 0 0 1 12 0Zm-1 15V8h2v7h-2Zm2.5 3.5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Z" fill="currentColor"></path>
        {% } %}
        {% if (ctx.component.alerttype === "error" || ctx.component.alerttype === "feil") { %}
        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0Zm3.571 7L17 8.429 13.428 12 17 15.571 15.571 17 12 13.428 8.429 17 7 15.571 10.572 12 7 8.429 8.429 7 12 10.572 15.571 7Z" fill="currentColor"></path>
        {% } %}
    </svg>
    <div class="navds-alert__wrapper navds-body-long">{{ctx.t(ctx.content)}}</div>
</div>