<div class="navds-checkbox navds-checkbox--medium form-check checkbox">
  <{{ctx.input.type}}
  ref="input"
  id="{{ctx.instance.id}}-{{ctx.component.key}}"
  class="navds-checkbox__input checkboks"
  {% for (var attr in ctx.input.attr) { %}
  {{attr}}="{{ctx.input.attr[attr]}}"
  {% } %}
  aria-describedby="{{ ctx.component.description ? `d-${ctx.instance.id}-${ctx.component.key} ` : '' }}e-{{ctx.instance.id}}-{{ctx.component.key}}"
  {% if (ctx.checked) { %}checked=true{% } %}
  />
  {% if (!ctx.self.labelIsHidden()) { %}
  <label class="navds-checkbox__label" for="{{ctx.instance.id}}-{{ctx.component.key}}">
    {{ctx.t(ctx.input.label)}}
    {% if(ctx.component.validate.required === false) { %}
    ({{ctx.t("valgfritt")}})
    {% } %}
  </label>
  {% } %}
  </div>
