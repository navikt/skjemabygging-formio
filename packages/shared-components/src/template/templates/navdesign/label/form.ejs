<label class="skjemaelement__label {{ctx.label.className}}" for="{{ctx.component.id}}-{{ctx.component.key}}" id="{{ctx.component.id}}-{{ctx.component.key}}-label">
  {{ ctx.t(ctx.component.label) }}
  {% if(ctx.component.validate.required === false) { %}
    ({{ctx.t("valgfritt")}})
  {% } %}
  {% if (ctx.component.tooltip) { %}
    <i ref="tooltip" class="{{ctx.iconClass('question-sign')}} text-muted"></i>
  {% } %}
</label>
