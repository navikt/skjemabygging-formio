<div class="skjemaelement form-check checkbox">
    <{{ctx.input.type}}
    ref="input"
    id="{{ctx.component.key}}"
    class="skjemaelement__input checkboks"
    {% for (var attr in ctx.input.attr) { %}
    {{attr}}="{{ctx.input.attr[attr]}}"
    {% } %}
    {% if (ctx.checked) { %}checked=true{% } %}>
        {{ctx.input.content}}
    </{{ctx.input.type}}>
    {% if (!ctx.self.labelIsHidden()) { %}
    <label class="skjemaelement__label {% if (ctx.component.validate.required) { %} field-required{% } %}" for="{{ctx.component.key}}">
        {{ctx.input.label}}
    </label>
    {% } %}
</div>
