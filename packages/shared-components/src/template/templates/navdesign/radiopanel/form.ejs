<fieldset class="navds-radio-group navds-radio-group--medium navds-fieldset navds-fieldset--medium" aria-labelledby="l-{{ctx.instance.id}}-{{ctx.component.key}}" aria-describedby="e-{{ctx.instance.id}}-{{ctx.component.key}} {% if (ctx.component.description) { %}d-{{ctx.instance.id}}-{{ctx.component.key}}{% } %}">
  <legend class="navds-fieldset__legend navds-label" id="l-{{ctx.instance.id}}-{{ctx.component.key}}">
    {{ctx.t(ctx.component.label)}}
    {% if(ctx.component.validate.required === false) { %}
      ({{ctx.t("valgfritt")}})
    {% } %}
  </legend>
  {% if (ctx.component.description && ctx.component.descriptionPosition !== "above") { %}
    <div class="navds-fieldset__description" id="d-{{ctx.instance.id}}-{{ctx.component.key}}">{{ctx.t(ctx.component.description)}}</div>
    {% if (ctx.component.additionalDescription) { %}
      <div class="accordion-link" data-testid="{{ctx.component.key}}-additional-description" id="{{ctx.component.id}}-{{ctx.component.key}}-additional-description">
        {% if (ctx.component.additionalDescriptionLabel) { %}
          <button type="button" class="accordion-link__link" onclick="{{ctx.utils.toggleClass(`${ctx.component.id}-${ctx.component.key}-additional-description`, 'accordion-link--expand')}}">
            {{ctx.t(ctx.component.additionalDescriptionLabel)}}
          </button>
        {% } %}
        <div class="accordion-link__text">{{ctx.t(ctx.component.additionalDescriptionText)}}</div>
      </div>
    {% } %}
  {% } %}
  <div class="navds-radio-buttons">
  {% ctx.values.forEach(function(item) { %}
  <div class="navds-radio navds-radio--medium" ref="wrapper">
    <{{ctx.input.type}}
      class="navds-radio__input radioknapp"
      ref="input"
      {% for (var attr in ctx.input.attr) { %}
      {{attr}}="{{ctx.input.attr[attr]}}"
      {% } %}
      value="{{item.value}}"
      {% if (ctx.value && (ctx.value === item.value || (typeof ctx.value === 'object' && ctx.value.hasOwnProperty(item.value) && ctx.value[item.value]))) { %}
      checked=true
      {% } %}
      {% if (item.disabled) { %}
      disabled=true
      {% } %}
      id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
    >
    <label class="navds-radio__label" for="{{ctx.id}}{{ctx.row}}-{{item.value}}">{{ctx.t(item.label)}} </label>
  </div>
  {% }) %}
  </div>
</fieldset>
