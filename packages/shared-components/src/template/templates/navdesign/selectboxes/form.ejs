<fieldset class="skjemagruppe inputPanelGruppe">
  <legend class="skjemagruppe__legend">
    {{ctx.t(ctx.component.label)}}
    {% if(ctx.component.validate.required === false) { %}
      ({{ctx.t("valgfritt")}})
    {% } %}
  </legend>
  {% if (ctx.component.description && ctx.component.descriptionPosition !== "above" ) { %}
    <div class="skjemagruppe__description">{{ctx.t(ctx.component.description)}}</div>
    {% if (ctx.component.additionalDescription) { %}
      <div class="accordion-link" data-testid="{{ctx.component.key}}-additional-description" id="{{ctx.component.id}}-{{ctx.component.key}}-additional-description">
        {% if (ctx.component.additionalDescriptionLabel) { %}
          <button type="button" class="accordion-link__link" onclick="{{ctx.utils.toggleClass(`${ctx.component.id}-${ctx.component.key}-additional-description`, 'accordion-link--expand')}}">
            {{ctx.t(ctx.component.additionalDescriptionLabel)}}
          </button>
        {% } %}
        <div class="accordion-link__text">{{ctx.t(ctx.component.additionalDescriptionText)}}</div>
      </div>
    {% } %}
  {% } %}
  {% ctx.values.forEach(function(item) { %}
    <div class="skjemaelement form-check checkbox">
      <input
        ref="input"
        type="checkbox"
        id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
        class="skjemaelement__input checkboks"
        name="data[{{ctx.key}}][{{item.value}}]"
        {% for (var attr in ctx.input.attr) { %}
        {{attr}}="{{ctx.input.attr[attr]}}"
        {% } %}
        aria-checked="{{ctx.value[item.value] === true}}"
        aria-describedby="{{ctx.key}}-error"
        value="{{item.value}}"
        {% if (ctx.value[item.value] === true) { %}
        checked
        {% } %}
        {% if (item.disabled) { %}
        disabled
        {% } %}
      />
      <label class="skjemaelement__label" for="{{ctx.id}}{{ctx.row}}-{{item.value}}">{{ctx.t(item.label)}}</label>
    </div>
  {% }) %}
</fieldset>