<div class="{{ctx.className}}">
  <div>
    <main id="maincontent" class="fyllut-layout" tabIndex="-1">
      <div class="main-col">
        <h2 id="form-id" class="margin-bottom-default typo-innholdstittel">{{ctx.t(ctx.panels[ctx.currentPage].title)}}</h2>
        <form aria-labelledby="form-id">
          <div class="wizard-page" ref="{{ctx.wizardKey}}">
            {{ ctx.components }}
          </div>
          <div class="margin-bottom-default">
            {{ ctx.wizardNav }}
          </div>
        </form>
      </div>
      <aside class="right-col">
        {{ ctx.wizardHeader }}
      </aside>
    </main>
  </div>
</div>
