<div>
{% if (ctx.submission?.fyllutState?.mellomlagring?.savedDate) { %}
  <p class="navigation-detail">{{ctx.t('Sist lagret') + ' ' + ctx.submission.fyllutState.mellomlagring.savedDate}}</p>
{% } %}
  <nav id="{{ ctx.wizardKey }}-nav">
  {% if ((ctx.buttons.next || ctx.buttons.submit) && ctx.buttons.previous) { %}
    <div class="button-row button-row__center">
  {% } else { %}
    <div class="button-row">
  {% } %}
    {% if (ctx.buttons.next) { %}
      <button class="navds-button navds-button--primary" type="submit"
        ref="{{ctx.wizardKey}}-next">
        <span aria-live="polite" class="navds-label">{{ctx.t('Neste steg')}}</span>
      </button>
    {% } %}
    {% if (ctx.buttons.submit) { %}
      <button class="navds-button navds-button--primary" type="submit"
        ref="{{ctx.wizardKey}}-submit">
        <span aria-live="polite" class="navds-label">{{ctx.t('Neste steg')}}</span>
      </button>
    {% } %}
    {% if (ctx.buttons.previous) { %}
      <button class="navds-button navds-button--secondary" type="button"
        ref="{{ctx.wizardKey}}-previous">
        <span aria-live="polite" class="navds-label">{{ctx.t('Forrige steg')}}</span>
      </button>
    {% } %}
    </div>
  {% if ((ctx.buttons.next || ctx.buttons.submit) && ctx.buttons.previous) { %}
    <div class="button-row button-row__center">
  {% } else { %}
    <div class="button-row">
  {% } %}
  {% if (ctx.buttons.cancel) { %}
      <button class="navds-button navds-button--tertiary" ref="{{ctx.wizardKey}}-cancel" type="button">
        <span aria-live="polite" class="navds-label">
      {% if (ctx.submission?.fyllutState?.mellomlagring?.isActive) { %}
            {{ctx.t('Avbryt og lagre')}}
      {% } else { %}
            {{ctx.t('Avbryt og forkast')}}
      {% } %}
        </span>
      </button>
  {% } %}
    </div>
  </nav>
</div>
