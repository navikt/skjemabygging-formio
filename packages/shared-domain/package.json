{
  "name": "@navikt/skjemadigitalisering-shared-domain",
  "version": "1.0.0",
  "description": "Kode som er delt mellom bygger og utfyller",
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/skjemabygging-formio.git",
    "directory": "packages/shared-domain"
  },
  "main": "dist/index.js",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts,.js\"  --ignore \"**/*.test.js,**/*.test.ts,**/testdata/**\"",
    "check-types": "tsc",
    "clean": "rimraf dist coverage",
    "watch": "babel -w src --out-dir dist --extensions \".ts,.js\"  --ignore \"**/*.test.js,**/*.test.ts,**/testdata/**\"",
    "prepare": "yarn build && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "jest": "^27.0.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "formiojs": "4.14.0-rc.18"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "uuid": "^8.3.2"
  }
}
