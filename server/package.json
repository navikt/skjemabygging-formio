{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "config": {
    "icuDataDir": "./node_modules/full-icu",
    "decoratorUrl": "https://www.nav.no/dekoratoren/",
    "foersteSideUrl": "https://www.nav.no/soknader/api/forsteside"
  },
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development FOERSTESIDE_URL=$npm_package_config_foersteSideUrl DECORATOR_URL=$npm_package_config_decoratorUrl nodemon --icu-data-dir=$npm_package_config_icuDataDir $npm_package_main",
    "start-prod": "cross-env node --icu-data-dir=$npm_package_config_icuDataDir $npm_package_main",
    "test": "cross-env node --icu-data-dir=$npm_package_config_icuDataDir --experimental-vm-modules node_modules/jest/bin/jest.js",
    "check-env": "node -e 'console.log(process.env)' | grep npm"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {}
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "full-icu": "^1.3.1",
    "jsdom": "^16.4.0",
    "luxon": "^1.25.0",
    "mustache-express": "^1.3.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.0",
    "pdfmake": "^0.1.68",
    "prom-client": "^12.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jest": "^26.4.2",
    "nodemon": "^2.0.6"
  }
}
