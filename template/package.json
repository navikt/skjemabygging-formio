    "pre-commit": "^1.2.2",
    "sinon": "^9.0.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/semantic.git"
  },
  "author": "randall@form.io",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.10.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^5.0.0",
    "webpack": "^4.42.0"
  }
