{
  "name": "navdesign",
  "version": "0.0.1",
  "description": "NAV design templates for form.io forms.",
  "main": "lib/index.js",
  "scripts": {
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "mocha --require node/register --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "build": "gulp transpile-js && gulp templates && webpack && gulp move-to-node_modules",
    "lint": "tslint -p ."
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "UI",
    "form.io",
    "forms"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
