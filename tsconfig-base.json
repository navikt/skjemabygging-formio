{
  "compilerOptions": {
    "target": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "types": [
      "vite/client",
      "vitest/globals"
    ],
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
  }
}
